{
  "languageserver": {
    "ocaml": {
      "command": "opam",
      "args": ["exec", "--", "ocamllsp"],
      "filetypes": ["ocaml", "reason"]
    },
    "haskell": {
      "command": "haskell-language-server-wrapper",
      "args": ["--lsp"],
      "rootPatterns": [
        "*.cabal",
        "stack.yaml",
        "cabal.project",
        "package.yaml",
        "hie.yaml"
      ],
      "filetypes": ["haskell", "lhaskell"]
    }
  },
  "java.configuration.runtimes": [
    {
      "name": "JavaSE-11",
      "path": "/usr/lib/jvm/java-11-openjdk"
    },
    {
      "name": "JavaSE-18",
      "path": "/usr/lib/jvm/java-18-openjdk",
      "default": true
    }
  ],
  "java.jdt.ls.vmargs": "-Dfile.encoding=UTF-8",
  "java.import.gradle.enabled": true,
  "java.import.gradle.java.home": "/usr/lib/jvm/java-18-openjdk",
  "java.import.gradle.wrapper.enabled": false,
  "java.import.gradle.offline.enabled": true,
  "java.import.gradle.home": "/usr/share/java/gradle",
  "codeLens.enable": true,
  "workspace.workspaceFolderCheckCwd": false,
  "list.source.mru.ignoreGitIgnore": true,
  "coc.preferences.enableMessageDialog": false,
  "notification.disabledProgressSources": ["*"],
  "texlab.build.onSave": true
}
